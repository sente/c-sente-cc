find tmp/daily/{03,04} . -maxdepth 3 -type f -name "*.py" -mtime -1 |while read line; do canonpath $line; done |while read line; do dirname $line;done |uniq | while read line; do echo $line; find $line -maxdepth 1 -type f -mtime -1 -name "*.py"|xargs -n1 -d'\n' basename | sed 's/^/\t|_____->/'; echo; done | postit today.txt;
